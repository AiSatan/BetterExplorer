<Window x:Class="BetterExplorer.Tabs.TabManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Tab Manager" Height="400" Width="600" xmlns:my="clr-namespace:BetterExplorer">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="138*" />
            <ColumnDefinition Width="440*" />
        </Grid.ColumnDefinitions>
        <my:TabListEditor HorizontalAlignment="Left" Margin="0,31,0,0" x:Name="tabListEditor1" VerticalAlignment="Top" Height="330" Width="440" Grid.Column="1" />
        <StackPanel Name="stackPanel1" Margin="0,0,4,0" />
        <GridSplitter Name="gridSplitter1" HorizontalAlignment="Right" Width="4" Background="#FFEEEEEE" />
    </Grid>
</Window>
