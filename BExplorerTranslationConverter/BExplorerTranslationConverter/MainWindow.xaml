        <Button Content="Edit Value..." Height="23" HorizontalAlignment="Left" Margin="14,0,0,27" Name="Button1" VerticalAlignment="Bottom" Width="75" />
        <Button Content="Add Value..." Height="23" HorizontalAlignment="Left" Margin="95,0,0,27" Name="Button2" VerticalAlignment="Bottom" Width="75" />
