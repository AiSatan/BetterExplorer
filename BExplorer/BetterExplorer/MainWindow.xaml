<Ribbon:RibbonWindow
                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero"
                     xmlns:Ribbon="clr-namespace:Fluent;assembly=Fluent"
                     xmlns:controls="clr-namespace:BetterExplorerControls;assembly=BetterExplorerControls"
                     xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                     xmlns:local="clr-namespace:BetterExplorer"
                     xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                     xmlns:my2="http://schemas.microsoft.com/winfx/2006/xaml/presentation/ribbon"
                     xmlns:ni="http://www.hardcodet.net/taskbar"
                     xmlns:console="clr-namespace:ConsoleControl;assembly=ConsoleControl"
                     xmlns:Controls="clr-namespace:wyDay.Controls;assembly=AutomaticUpdaterWPF" x:Class="BetterExplorer.MainWindow"
                     Title="Better Explorer"
                     Activated="RibbonWindow_Activated"
                     Closing="RibbonWindow_Closing"
                     Deactivated="RibbonWindow_Deactivated"
                     GotFocus="RibbonWindow_GotFocus"
                     GotKeyboardFocus="RibbonWindow_GotKeyboardFocus"
                     Initialized="RibbonWindow_Initialized"
                     KeyUp="RibbonWindow_KeyUp"
                     Loaded="Window_Loaded"
                     Background="{DynamicResource WindowBackgroundBrush}"
                     MouseRightButtonUp="RibbonWindow_MouseRightButtonUp"
                     
                     ResizeMode="CanResizeWithGrip"
                     TextOptions.TextFormattingMode="Display"
                     WindowState="Maximized"
                     d:DesignHeight="768"
                     d:DesignWidth="852"
                     mc:Ignorable="d" SizeChanged="RibbonWindow_SizeChanged" MinWidth="640" MinHeight="480" >

    <Ribbon:RibbonWindow.Resources>
        <ResourceDictionary>
            <ControlTemplate x:Key="HorizontalScrollBarTemplate" TargetType="{x:Type ScrollBar}">
                <Grid x:Name="Bg"
                      Background="Transparent"
                      SnapsToDevicePixels="True">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition MaxWidth="{DynamicResource {x:Static SystemParameters.HorizontalScrollBarButtonWidthKey}}" />
                        <ColumnDefinition Width="1E-05*" />
                        <ColumnDefinition MaxWidth="{DynamicResource {x:Static SystemParameters.HorizontalScrollBarButtonWidthKey}}" />
                    </Grid.ColumnDefinitions>
                    <RepeatButton Command="ScrollBar.LineLeftCommand"
                                  IsEnabled="{TemplateBinding IsMouseOver}"
                                  Microsoft_Windows_Themes:ScrollChrome.ScrollGlyph="LeftArrow">
                        <RepeatButton.Style>
                            <Style TargetType="{x:Type RepeatButton}">
                                <Setter Property="OverridesDefaultStyle" Value="True" />
                                <Setter Property="Focusable" Value="False" />
                                <Setter Property="IsTabStop" Value="False" />
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="{x:Type RepeatButton}">
                                            <Border Width="18"
                                                    Height="18"
                                                    Background="{x:Null}">
                                                <Path Data=" M 8 15 L 8 3 L 2 9 Z" Fill="Black" />
                                            </Border>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </RepeatButton.Style>
                    </RepeatButton>
                    <RepeatButton Grid.Column="2"
                                  Command="ScrollBar.LineRightCommand"
                                  IsEnabled="{TemplateBinding IsMouseOver}"
                                  Microsoft_Windows_Themes:ScrollChrome.ScrollGlyph="RightArrow">
                        <RepeatButton.Style>
                            <Style TargetType="{x:Type RepeatButton}">
                                <Setter Property="OverridesDefaultStyle" Value="True" />
                                <Setter Property="Focusable" Value="False" />
                                <Setter Property="IsTabStop" Value="False" />
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="{x:Type RepeatButton}">
                                            <Border Width="18"
                                                    Height="18"
                                                    Background="{x:Null}">
                                                <Path Data=" M 9 15 L 9 3 L 15 9 Z" Fill="Black" />
                                            </Border>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </RepeatButton.Style>
                    </RepeatButton>
                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="IsEnabled" Value="False">
                        <Setter TargetName="Bg" Property="Background" Value="#FFF4F4F4" />
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>
            <ControlTemplate x:Key="ScrollViewerTemplate" TargetType="{x:Type ScrollViewer}">
                <Grid x:Name="Grid" Background="Transparent">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Rectangle x:Name="Corner"
                               Grid.Row="1"
                               Grid.Column="1"
                               Fill="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" />
                    <ScrollBar x:Name="PART_HorizontalScrollBar"
                               Grid.Row="0"
                               Grid.Column="0"
                               AutomationProperties.AutomationId="HorizontalScrollBar"
                               Cursor="Arrow"
                               Maximum="{TemplateBinding ScrollableWidth}"
                               Minimum="0"
                               Orientation="Horizontal"
                               Template="{DynamicResource HorizontalScrollBarTemplate}"
                               ViewportSize="{TemplateBinding ViewportWidth}"
                               Visibility="{TemplateBinding ComputedHorizontalScrollBarVisibility}"
                               Value="{Binding HorizontalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" />
                    <ScrollContentPresenter x:Name="PART_ScrollContentPresenter"
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            Margin="0,2,0,0"
                                            CanContentScroll="{TemplateBinding CanContentScroll}"
                                            CanHorizontallyScroll="False"
                                            CanVerticallyScroll="False"
                                            Content="{TemplateBinding Content}"
                                            ContentTemplate="{TemplateBinding ContentTemplate}" />
                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="ComputedHorizontalScrollBarVisibility" Value="Visible">
                        <Setter TargetName="PART_ScrollContentPresenter" Property="Margin" Value="15,2,15,0" />
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>
            <ControlTemplate x:Key="ScrollableTabControlTemplate" TargetType="{x:Type TabControl}">
                <Grid ClipToBounds="True"
                      KeyboardNavigation.TabNavigation="Local"
                      SnapsToDevicePixels="True">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition x:Name="ColumnDefinition0" />
                        <ColumnDefinition x:Name="ColumnDefinition1" Width="0" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition x:Name="RowDefinition0" Height="Auto" />
                        <RowDefinition x:Name="RowDefinition1" Height="*" />
                    </Grid.RowDefinitions>

                    <Border x:Name="ContentPanel"
                            Grid.Row="1"
                            Grid.Column="0"
                            Margin="0,0,0,0"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="0,1,0,0"
                            KeyboardNavigation.DirectionalNavigation="Contained"
                            KeyboardNavigation.TabIndex="2"
                            KeyboardNavigation.TabNavigation="Local">
                        <ContentPresenter x:Name="PART_SelectedContentHost"
                                          Margin="{TemplateBinding Padding}"
                                          Content="{TemplateBinding SelectedContent}"
                                          ContentSource="SelectedContent"
                                          ContentStringFormat="{TemplateBinding SelectedContentStringFormat}"
                                          ContentTemplate="{TemplateBinding SelectedContentTemplate}"
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                    </Border>

                    <ScrollViewer x:Name="svTabBar" Margin="0,0,0,-1"
                                  HorizontalScrollBarVisibility="Auto"
                                  PreviewMouseWheel="ScrollViewer_PreviewMouseWheel"
                                  Template="{DynamicResource ScrollViewerTemplate}"
                                  VerticalScrollBarVisibility="Disabled" MouseWheel="svTabBar_MouseWheel">
                        <TabPanel x:Name="HeaderPanel"
                                  Margin="0,2,2,1"
                                  IsItemsHost="True"
                                  KeyboardNavigation.TabIndex="1" />
                    </ScrollViewer>


                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="TabStripPlacement" Value="Bottom">
                        <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="1" />
                        <Setter TargetName="ContentPanel" Property="Grid.Row" Value="0" />
                        <Setter TargetName="RowDefinition0" Property="Height" Value="*" />
                        <Setter TargetName="RowDefinition1" Property="Height" Value="Auto" />
                        <Setter TargetName="HeaderPanel" Property="Margin" Value="2,0,2,2" />
                    </Trigger>
                    <Trigger Property="TabStripPlacement" Value="Left">
                        <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="0" />
                        <Setter TargetName="ContentPanel" Property="Grid.Row" Value="0" />
                        <Setter TargetName="HeaderPanel" Property="Grid.Column" Value="0" />
                        <Setter TargetName="ContentPanel" Property="Grid.Column" Value="1" />
                        <Setter TargetName="ColumnDefinition0" Property="Width" Value="Auto" />
                        <Setter TargetName="ColumnDefinition1" Property="Width" Value="*" />
                        <Setter TargetName="RowDefinition0" Property="Height" Value="*" />
                        <Setter TargetName="RowDefinition1" Property="Height" Value="0" />
                        <Setter TargetName="HeaderPanel" Property="Margin" Value="2,2,0,2" />
                    </Trigger>
                    <Trigger Property="TabStripPlacement" Value="Right">
                        <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="0" />
                        <Setter TargetName="ContentPanel" Property="Grid.Row" Value="0" />
                        <Setter TargetName="HeaderPanel" Property="Grid.Column" Value="1" />
                        <Setter TargetName="ContentPanel" Property="Grid.Column" Value="0" />
                        <Setter TargetName="ColumnDefinition0" Property="Width" Value="*" />
                        <Setter TargetName="ColumnDefinition1" Property="Width" Value="Auto" />
                        <Setter TargetName="RowDefinition0" Property="Height" Value="*" />
                        <Setter TargetName="RowDefinition1" Property="Height" Value="0" />
                        <Setter TargetName="HeaderPanel" Property="Margin" Value="0,2,2,2" />
                    </Trigger>
                    <Trigger Property="IsEnabled" Value="False">
                        <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}" />
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>
		    </ResourceDictionary>
    </Ribbon:RibbonWindow.Resources>

    <Grid x:Name="grid1">
        <Grid.RowDefinitions>
            <RowDefinition x:Name="rRibbon" Height="Auto" />
            <RowDefinition x:Name="rCommands" Height="36" />
            <RowDefinition x:Name="NavPanelDivider" Height="Auto"/>
            <RowDefinition x:Name="rTabbarTop" Height="25" />
            <RowDefinition Height="*" />
            <RowDefinition x:Name="spCommandPrompt" Height="Auto"/>
            <RowDefinition x:Name="rCommandPrompt" Height="*"/>
            <RowDefinition x:Name="rTabbarBot" Height="0" />
            <RowDefinition x:Name="rStatusbar" Height="20" />
        </Grid.RowDefinitions>

        <ni:TaskbarIcon x:Name="beNotifyIcon"
                    Visibility="Visible"
                    ToolTipText="Better Explorer (A better filemanager for windows)"
                    IconSource="Explorer.ico"
                    MenuActivation="RightClick"
                    PopupActivation="DoubleClick" TrayMouseDoubleClick="beNotifyIcon_TrayMouseDoubleClick">

            <ni:TaskbarIcon.ContextMenu>
                <ContextMenu>
                    <MenuItem Header="Show Window" Click="beNotifyIcon_TrayMouseDoubleClick"  />
                    <MenuItem Header="Exit" Click="btnBackstageExit_Click" />
                </ContextMenu>
            </ni:TaskbarIcon.ContextMenu>
        </ni:TaskbarIcon>
        
    <Ribbon:Ribbon x:Name="TheRibbon"
                       Grid.ColumnSpan="3"
                       Background="White"
                       CustomizeQuickAccessToolbar="TheRibbon_CustomizeQuickAccessToolbar"
                       Focusable="False"
                       GotFocus="TheStatusBar_GotFocus"
                       IsCollapsedChanged="TheRibbon_IsCollapsedChanged"
                       SizeChanged="TheRibbon_SizeChanged" ItemAddedToQuickAccessToolbar="TheRibbon_ItemAddedToQuickAccessToolbar" ItemRemovedToQuickAccessToolbar="TheRibbon_ItemRemovedToQuickAccessToolbar" CanCustomizeQuickAccessToolBar="True">
            <Ribbon:Ribbon.ToolBarItems>
                <Ribbon:Button x:Name="btnAbout"
                               Click="btnAbout_Click"
                               Icon="Images\about16.png"
                               Ribbon:KeyTip.Keys="B"
                               SizeDefinition="Small,Small,Small"
                               Visibility="Collapsed" />
            </Ribbon:Ribbon.ToolBarItems>

            <Ribbon:Ribbon.QuickAccessItems>
                <Ribbon:QuickAccessMenuItem IsChecked="True" Target="{Binding ElementName=btnCut}" Focusable="False"/>
                <Ribbon:QuickAccessMenuItem IsChecked="True" Target="{Binding ElementName=btnCopy}" Focusable="False"/>
                <Ribbon:QuickAccessMenuItem IsChecked="True" Target="{Binding ElementName=btnPaste}" Focusable="False"/>
                <Ribbon:QuickAccessMenuItem IsChecked="True" Target="{Binding ElementName=btnNewTab}" Focusable="False"/>
                <Ribbon:QuickAccessMenuItem IsChecked="True" Target="{Binding ElementName=btnTabClone}" Focusable="False"/>
                <Ribbon:QuickAccessMenuItem IsChecked="False" Target="{Binding ElementName=btnNewWindow}" Focusable="False"/>
                <Ribbon:QuickAccessMenuItem IsChecked="False" Target="{Binding ElementName=btnNavigationPane}" Focusable="False"/>
                <Ribbon:QuickAccessMenuItem IsChecked="False" Target="{Binding ElementName=btnPreviewPane}" Focusable="False"/>
                <Ribbon:QuickAccessMenuItem IsChecked="False" Target="{Binding ElementName=btnInfoPane}" Focusable="False"/>
            </Ribbon:Ribbon.QuickAccessItems>

            <Ribbon:Ribbon.Menu>
                <Ribbon:Backstage x:Name="backstage"
                                  Background="DarkOrange"
                                  FontStretch="Expanded"
                                  FontStyle="Normal"
                                  FontWeight="Normal"
                                  IsOpenChanged="backstage_IsOpenChanged"
                                  Ribbon:KeyTip.Keys="F" Focusable="False">
                    <Ribbon:BackstageTabControl>
                        <Ribbon:Button x:Name="btnNewWindow"
                                       Click="Button_Click_5"
                                       Content="New Window"
                                       Ribbon:KeyTip.Keys="N"
                                       Header="{StaticResource btnNewWindowCP}"
                                       Icon="Images\new_window.png" />
                        <Ribbon:BackstageTabItem Header="{StaticResource btiInformationCP}" Ribbon:KeyTip.Keys="I"  Visibility="Visible">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2*" />
                                    <ColumnDefinition Width="3*" />
                                </Grid.ColumnDefinitions>
                                <Ribbon:SeparatorTabItem Margin="15,10,10,0"
                                                         VerticalAlignment="Top"
                                                         Header="{StaticResource mmSupportCP}"
                                                         Style="{DynamicResource InnerBackstageSeparatorTabItemStyle}" />
                                <Ribbon:Button x:Name="btnUpdateCheck"
                                               Height="88"
                                               Margin="20,45,0,0"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Top"
                                               Click="Button_Click_6"
                                               Header="{StaticResource btnUpdateCheckCP}"
                                               Icon="Images\updater16.png"
                                               IsDefinitive="True"
                                               LargeIcon="Images\updater32.png"
                                               Ribbon:KeyTip.Keys="U"
                                               Ribbon:KeyTip.AutoPlacement="False"
                                               Ribbon:KeyTip.VerticalAlignment="Bottom"
                                               Ribbon:KeyTip.HorizontalAlignment="Center"
                                               Style="{DynamicResource ButtonBackstageStyle}">
                                    <Ribbon:Button.ToolTip>
                                        <Ribbon:ScreenTip Title="{StaticResource btnUpdateCheckHCP}"
                                                          Width="250"
                                                          DisableReason="Only one window of BetterExplorer can manage updates at any time. Another window may already be managing updates, and you can check for updates on that window. If no other window is open, open a new window to check for updates."
                                                          IsRibbonAligned="false"
                                                          Text="{StaticResource btnUpdateCheckICP}" />
                                    </Ribbon:Button.ToolTip>
                                </Ribbon:Button>
                                <Label Margin="105,45,10,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="{StaticResource btnUpdateCheckHCP}"
                                       FontSize="12"
                                       FontWeight="Bold" />
                                <TextBlock Height="50"
                                           Margin="110,75,10,0"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Top"
                                           Text="{StaticResource btnUpdateCheckICP}"
                                           TextWrapping="Wrap" />
                                <Ribbon:Button Height="88"
                                               Margin="20,140,0,0"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Top"
                                               Click="Button_Click_7"
                                               Header="{StaticResource btnOpenWebsite}"
                                               Icon="Images\webconnect_16.png"
                                               IsDefinitive="True"
                                               LargeIcon="Images\webconnect_32.png"
                                               Ribbon:KeyTip.Keys="W"
                                               Ribbon:KeyTip.AutoPlacement="False"
                                               Ribbon:KeyTip.VerticalAlignment="Bottom"
                                               Ribbon:KeyTip.HorizontalAlignment="Center"
                                               Style="{DynamicResource ButtonBackstageStyle}" />
                                <Label Margin="105,140,10,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="{StaticResource btnOpenWebsiteHCP}"
                                       FontSize="12"
                                       FontWeight="Bold" />
                                <TextBlock Height="50"
                                           Margin="110,170,10,0"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Top"
                                           Text="{StaticResource btnOpenWebsiteICP}"
                                           TextWrapping="Wrap" />
                                <Ribbon:Button Height="88"
                                               Margin="20,235,0,0"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Top"
                                               Click="btnBugtracker_Click"
                                               Header="Report a problem"
                                               Icon="Images\bugtrac_16.png"
                                               IsDefinitive="True"
                                               LargeIcon="Images\bugtrac_32.png"
                                               Ribbon:KeyTip.Keys="R"
                                               Ribbon:KeyTip.AutoPlacement="False"
                                               Ribbon:KeyTip.VerticalAlignment="Bottom"
                                               Ribbon:KeyTip.HorizontalAlignment="Center"
                                               Style="{DynamicResource ButtonBackstageStyle}" />
                                <Label Margin="105,235,10,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Content="Report a problem, bug or feature request"
                                       FontSize="12"
                                       FontWeight="Bold" />
                                <TextBlock Height="50"
                                           Margin="110,265,10,0"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Top"
                                           Text="This redirects you to the bugtracker website where you can post issues."
                                           TextWrapping="Wrap" />
                                <Separator Grid.Column="0" HorizontalAlignment="Right" />
                                <Image Grid.Column="1"
                                       Width="400"
                                       Height="100"
                                       Margin="10,20,0,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Source="Images\bexplorerlogo.png"
                                       Stretch="Fill" />
                                <ScrollViewer Grid.Column="1"
                                              Margin="20,110,10,0"
                                              HorizontalScrollBarVisibility="Auto"
                                              VerticalScrollBarVisibility="Auto">
                                    <StackPanel Margin="1,0,0,1">
                                        <Controls:AutomaticUpdater x:Name="autoUpdater" Margin="0,15,50,10" HorizontalAlignment="Left" UpdateType="OnlyCheck" Animate="True" Panel.ZIndex="100" Focusable="False" KeepExpanded="True" DaysBetweenChecks="7"/>
                                        <Label x:Name="stiUpdate"
                                               Margin="0,25,50,0"
                                               Content="{StaticResource stUpdateNotAvailableCP}"
                                               FontSize="14"
                                               FontWeight="ExtraBold" Visibility="Collapsed"/>
                                        <Ribbon:SeparatorTabItem Margin="0,-30,50,0"
                                                                 Foreground="#FFC71212"
                                                                 Header=""
                                                                 Style="{DynamicResource InnerBackstageSeparatorTabItemStyle}" />
                                        <Label Margin="0,2,50,0"
                                               Content="{StaticResource lblBetterExText}"
                                               FontWeight="Bold" />
                                        <TextBlock Width="500"
                                                   Margin="3,2,53,0"
                                                   HorizontalAlignment="Left"
                                                   Text="{StaticResource lblBetterExInfo}"
                                                   TextWrapping="Wrap" />
                                        <Ribbon:SeparatorTabItem Margin="0,20,50,0"
                                                                 Header="{StaticResource lblDevTeamCP}"
                                                                 Style="{DynamicResource InnerBackstageSeparatorTabItemStyle}" />
                                        <Label Margin="0,2,50,0"
                                               Content="{StaticResource lblDevelopersHeader}"
                                               FontWeight="Bold" />
                                        <Label Margin="20,0,40,0"
                                               Content="Dimitar Tsenev"
                                               FontSize="14"
                                               FontStyle="Italic"
                                               FontWeight="Bold" />
                                        <Label Margin="20,0,40,0"
                                               Content="Jayke R. Huempfner (JaykeBird)"
                                               FontSize="14"
                                               FontStyle="Italic"
                                               FontWeight="Bold" />
                                        <Label Margin="20,0,40,0"
                                               Content="Sebastien Kerguen"
                                               FontSize="14"
                                               FontStyle="Italic"
                                               FontWeight="Bold" />
                                        <Label Margin="20,0,40,0"
                                               Content="Markus Lang"
                                               FontSize="14"
                                               FontStyle="Italic"
                                               FontWeight="Bold" />
                                        <Label Margin="20,0,40,0"
                                               Content="Alexey Barkalov"
                                               FontSize="14"
                                               FontStyle="Italic"
                                               FontWeight="Bold" />
                                        <Ribbon:SeparatorTabItem Margin="0,20,50,0"
                                                                 Header="{StaticResource lblAboutBEHeader}"
                                                                 Style="{DynamicResource InnerBackstageSeparatorTabItemStyle}" />
                                        <Label x:Name="verNumber"
                                               Margin="20,0,50,0"
                                               Content="Version Info Data Put Here"
                                               FontSize="12" />
                                        <Label x:Name="lblArchitecture"
                                               Margin="20,0,0,0"
                                               Content="32-bit Version"
                                               FontSize="12" />
                                        <Label Margin="20,0,0,0" Content="© 2012. All right reserved." />
                                        <Ribbon:SeparatorTabItem Margin="0,20,50,0"
                                                                 Header="Credits"
                                                                 Style="{DynamicResource InnerBackstageSeparatorTabItemStyle}" />
                                        <Label Content="Fluent Ribbon Control Suite (fluent.codeplex.com)" />
                                        <!--  <Label Content="Qttabbar - a great utility for Windows Explorer"/>  -->
                                        <Label Content="7-Zip - archiving utility (www.7-zip.org)" />
                                        <Label Content="7ZipSharp - a wrapper of 7Zip for .NET" />
                                        <Label Content="Some icons and ExplorerBrowser control are property of Microsoft® Corporation. All right reserved." />

                                    </StackPanel>
                                </ScrollViewer>

                            </Grid>
                        </Ribbon:BackstageTabItem>
                        <Ribbon:BackstageTabItem Header="{StaticResource btiOptionsCP}" Ribbon:KeyTip.Keys="O">
                            <TabControl Margin="20,5,20,5" Style="{DynamicResource InnerBackstageTabControlStyle}">
                                <Ribbon:SeparatorTabItem Header="{StaticResource sepGeneral}" />
                                <TabItem Header="{StaticResource tabShell}"
                                         IsSelected="True"
                                         Ribbon:KeyTip.Keys="S">
                                    <StackPanel x:Name="ShellStack"
                                                Margin="20,5,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Top"
                                                Focusable="True"
                                                Orientation="Vertical">
                                        <Ribbon:SeparatorTabItem Header="{StaticResource sepStartup}" />
                                        <StackPanel Margin="0,5,0,0" Orientation="Horizontal">
                                            <!--
                                                Ribbon:TextBox Name="txtPath" Width="200"/>
                                                <Button Name="btnPathPick" Content="..." Width="22" HorizontalContentAlignment="Center" Height="22" Click="btnPathPick_Click" /
                                            -->
                                            <Border HorizontalAlignment="Stretch"
                                                    VerticalAlignment="Stretch"
                                                    BorderBrush="#FF000000"
                                                    BorderThickness="1">
                                                <Ribbon:Button x:Name="btnSetCurrentasStartup"
                                                               Width="Auto"
                                                               Margin="0"
                                                               BorderBrush="Black"
                                                               BorderThickness="1"
                                                               CanAddToQuickAccessToolBar="True"
                                                               Click="btnSetCurrentasStartup_Click"
                                                               Header="Libraries"
                                                               Padding="5,0,5,0"
                                                               Ribbon:KeyTip.Keys="S">
                                                    <Ribbon:Button.ToolTip>
                                                        <Ribbon:ScreenTip Title="{StaticResource btnSetCurrentasStartupHCP}"
                                                                          Width="250"
                                                                          IsRibbonAligned="false"
                                                                          Text="{StaticResource btnSetCurrentasStartupICP}" />
                                                    </Ribbon:Button.ToolTip>
                                                </Ribbon:Button>
                                            </Border>
                                            <Label Margin="3,0,0,0" Content="{StaticResource btnSetCurrentasStartupDesc}" />
                                        </StackPanel>
                                        <Label x:Name="paddinglbl1" Content="         " />
                                        <Ribbon:SeparatorTabItem Margin="0,0,0,15" Header="Other Settings" />
                                        <StackPanel Orientation="Horizontal">
                                            <Ribbon:CheckBox x:Name="chkIsDefault"
                                                             Checked="chkIsDefault_Checked"
                                                             Header="{StaticResource chkIsDefault}"
                                                             Ribbon:KeyTip.Keys="V"
                                                             Unchecked="chkIsDefault_Unchecked">
                                                <Ribbon:CheckBox.ToolTip>
                                                    <!--  Title="{StaticResource chkIsDefaultHCP}"  -->
                                                    <Ribbon:ScreenTip Title="{StaticResource chkIsDefault}"
                                                                      Width="250"
                                                                      DisableReason="A missing entry in Windows Registry keeps BetterExplorer from knowing or changing this setting. BetterExplorer will continue to work normally."
                                                                      Image="Images\uac48.png"
                                                                      IsRibbonAligned="false"
                                                                      Text="{StaticResource chkIsDefaultICP}" />
                                                </Ribbon:CheckBox.ToolTip>
                                            </Ribbon:CheckBox>
                                            <Image Source="Images\uac16.png" />
                                        </StackPanel>
                                        <Label x:Name="paddinglbl6" Content="         " />
                                        <Ribbon:CheckBox x:Name="chkIsTerraCopyEnabled"
                                                         Checked="chkIsTerraCopyEnabled_Checked"
                                                         Header="{StaticResource chkIsTerraCopyEnabled}"
                                                         Ribbon:KeyTip.Keys="T"
                                                         Unchecked="chkIsTerraCopyEnabled_Unchecked">
                                            <Ribbon:CheckBox.ToolTip>
                                                <!--  Title="{StaticResource chkIsTerraCopyEnabledHCP}"  -->
                                                <Ribbon:ScreenTip Title="{StaticResource chkIsTerraCopyEnabled}"
                                                                  Width="250"
                                                                  IsRibbonAligned="false"
                                                                  Text="{StaticResource chkIsTerraCopyEnabledICP}" />
                                            </Ribbon:CheckBox.ToolTip>
                                        </Ribbon:CheckBox>
